{
  "name": "td-element",
  "version": "0.1.12",
  "description": "Collect some useful components",
  "author": {
    "name": "panzhenyao"
  },
  "scripts": {
    "build:file": "node build/bin/iconInit.js & node build/bin/build-entry.js & node build/bin/version.js",
    "build:theme": "node build/bin/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "lint": "eslint src/**/* packages/**/* build/**/* --quiet",
    "dist": "npm run clean && npm run build:file && npm run lint && webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js && npm run build:theme"
  },
  "keyword": "td-element",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitee.com:tiandaai/td-element.git"
  },
  "main": "lib/td-element.common.js",
  "unpkg": "lib/index.js",
  "style": "lib/theme-chalk/index.css",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "2.6.14",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "3.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
    "@vue/cli-service": "^4.4.0",
    "babel-eslint": "^10.1.0",
    "cp-cli": "^1.0.2",
    "cross-env": "^3.1.3",
    "eslint": "6.7.2",
    "eslint-config-elemefe": "0.1.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-html": "^6.2.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "json-loader": "^0.5.7",
    "json-templater": "^1.0.4",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass": "^1.34.0",
    "sass-loader": "^10.1.1",
    "uppercamelcase": "^1.1.0",
    "rimraf": "^2.5.4",
    "vue-template-compiler": "2.6.14",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.11",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "license": "MIT"
}
